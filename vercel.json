{
  "name": "github-repository-analyzer",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_GITHUB_TOKEN": "@react_app_github_token",
    "REACT_APP_GEMINI_API_KEY": "@react_app_gemini_api_key"
  }
}
